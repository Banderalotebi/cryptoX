# Environment Variables
# Copy this file to .env and fill in your actual values

# RPC URL - Get from Alchemy, Infura, or other provider
RPC_URL=https://eth-goerli.g.alchemy.com/v2/your_api_key

# Private key of the wallet that will own the smart contract
PRIVATE_KEY=your_private_key_here

# Deployed wallet contract address (fill this after deployment)
WALLET_ADDRESS=0xYourDeployedWalletAddress

# Port for the backend server (optional)
PORT=3000
